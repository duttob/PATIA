(define (problem taquin-problem)
  (:domain taquin)
  (:objects
    x0 x1 x2 - coords
    y0 y1 y2 - coords
    t1 t2 t3 t4 t5 t6 t7 t8 - tile-obj
  )
  (:init
    (right-of x1 x0)
    (above y1 y0)
    (right-of x2 x1)
    (above y2 y1)
    (right-of x1 x0)
    (above y1 y0)
    (right-of x2 x1)
    (above y2 y1)
    (right-of x1 x0)
    (above y1 y0)
    (right-of x2 x1)
    (above y2 y1)
    (placed-at t6 x0 y0)
    (placed-at t3 x1 y0)
    (placed-at t7 x2 y0)
    (placed-at t5 x0 y1)
    (placed-at t8 x1 y1)
    (placed-at t4 x2 y1)
    (placed-at t2 x0 y2)
    (empty x1 y2)
    (placed-at t1 x2 y2)
  )
  (:goal (and
    (placed-at t1 x0 y0)
    (placed-at t2 x1 y0)
    (placed-at t3 x2 y0)
    (placed-at t4 x0 y1)
    (placed-at t5 x1 y1)
    (placed-at t6 x2 y1)
    (placed-at t7 x0 y2)
    (placed-at t8 x1 y2)
    (empty x2 y2)
  ))
)
